{
    "aws": {
        "region": "us-east-1"
    },
    "logging": {
        "logstash": {
            "host": "logger.aws.cbtnuggets.com",
            "port": 12201
        }
    },
    "statsd": {
        "host": "metrics.aws.cbtnuggets.com",
        "port": 8125
    },
    "rateLimiter": {
        "namespace": "node-template",
        "interval": 1000,
        "maxInInterval": 1
    },
    "api": {
        "http": {
            "host": "localhost",
            "port": 3000
        },
        "version": "v1",
        "prefix": "salesforce"
    },
    "mongodb": {
        "connectionType": "dnsResolve",
        "dbname": "playlist",
        "port": 27017,
        "replicaSet": "1"
    }
}
